{
  "name": "ai-chat-backend",
  "version": "1.0.0",
  "description": "Claude Desktop style AI Chat Backend",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "init:trial-chefs": "ts-node scripts/init-trial-chefs.ts",
    "import:recipes": "ts-node scripts/import-recipes.ts",
    "import:recipes:sequential": "ts-node scripts/index-recipes-sequential.ts",
    "generate:embeddings": "ts-node scripts/generate-embeddings.ts",
    "setup:complete": "npm run import:recipes && npm run generate:embeddings",
    "setup:complete:sequential": "ts-node scripts/setup-complete-sequential.ts complete",
    "index:sequential": "ts-node scripts/setup-complete-sequential.ts index",
    "embeddings:only": "ts-node scripts/setup-complete-sequential.ts embedding",
    "cleanup:descriptions": "ts-node scripts/cleanup-descriptions.ts",
    "setup:help": "ts-node scripts/setup-complete-sequential.ts help",
    "ollama:start": "bash scripts/start-ollama-optimized.sh"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.18.2",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.3.8",
    "@langchain/ollama": "^0.0.4",
    "@mongodb-js/mongodb-mcp-server": "^0.0.3",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.6",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/platform-socket.io": "^10.4.19",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/websockets": "^10.4.19",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.10.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.4.1",
    "langchain": "^0.2.5",
    "mongodb": "^6.17.0",
    "mongoose": "^8.4.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/testing": "^10.3.8",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^20.13.0",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
